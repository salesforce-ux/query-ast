{
  "name": "query-ast",
  "version": "1.0.0",
  "description": "A library to traverse/modify an AST",
  "main": "dist/index.js",
  "license": "SEE LICENSE IN README",
  "repository": {
    "type": "git",
    "url": "git@github.com:salesforce-ux/query-ast.git"
  },
  "scripts": {
    "start": "node --harmony_destructuring --harmony_rest_parameters lib/index.js",
    "test": "mocha lib/**/*.js test/**/*.js --reporter min --recursive --harmony_destructuring --harmony_rest_parameters",
    "lint": "standard",
    "build": "babel lib --out-dir dist --presets es2015",
    "doc": "jsdoc -c jsdoc.json && mv doc/query-ast/* doc/ && rm -rf doc/query-ast",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "invariant": "2.2.2",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "jsdoc": "3.4.3",
    "minami": "1.1.1",
    "mocha": "3.2.0",
    "scss-parser": "1.0.0",
    "standard": "8.6.0"
  },
  "standard": {
    "ignore": [
      "node_modules/**/*"
    ]
  }
}
